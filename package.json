{
  "name": "backbone-rel-partialput",
  "description": "Only send changed attributes / embeddings / references of backbone-rel models to the server",
  "keywords": [
    "backbone",
    "backbone-rel",
    "backbone-partialput"
  ],
  "author": "Jan-Felix Schwarz",
  "dependencies": {
    "underscore": ">=1.5.0",
    "backbone-rel": ">=0.10.0"
  },
  "devDependencies": {
    "docco": "0.6.3",
    "karma": "~0.12.31",
    "mocha": "~2.1.0",
    "karma-mocha": "~0.1.10",
    "requirejs": "~2.1.15",
    "karma-requirejs": "~0.2.2",
    "karma-phantomjs-launcher": "~0.1.4",
    "karma-coverage": "~0.2.7",
    "karma-junit-reporter": "~0.2.2",
    "jquery": "~2.1.3",
    "chai": "~1.10.0",
    "sinon": "~1.12.2",
    "sinon-chai": "~2.6.0",
    "uglify-js": "~2.4.16"
  },
  "scripts": {
    "test": "./node_modules/karma/bin/karma start karma.conf.js",
    "build": "./node_modules/uglify-js/bin/uglifyjs backbone-partialput.js --mangle --source-map backbone-partialput-min.js.map -o backbone-partialput-min.js",
    "doc": "./node_modules/docco/bin/docco backbone-partialput.js",
    "lint": "jsl -nofilelisting -nologo -conf docs/jsl.conf -process backbone-partialput.js"
  },
  "main": "backbone-partialput.js",
  "version": "0.10.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jfschwarz/backbone-partialput.git"
  }
}
